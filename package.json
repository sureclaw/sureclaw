{
  "name": "ax",
  "version": "0.1.0",
  "description": "Security-first personal AI agent",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "ax": "bin/ax.js"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:fuzz": "vitest run tests/ipc-fuzz.test.ts",
    "serve": "NODE_NO_WARNINGS=1 tsx src/cli/index.ts serve",
    "chat": "NODE_NO_WARNINGS=1 tsx src/cli/index.ts chat",
    "send": "NODE_NO_WARNINGS=1 tsx src/cli/index.ts send",
    "configure": "NODE_NO_WARNINGS=1 tsx src/cli/index.ts configure"
  },
  "keywords": [
    "ai",
    "agent",
    "security",
    "sandbox"
  ],
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.38",
    "@anthropic-ai/sdk": "^0.74.0",
    "@inquirer/prompts": "^8.2.0",
    "@mariozechner/pi-agent-core": "^0.52.9",
    "@mariozechner/pi-ai": "^0.52.9",
    "@mariozechner/pi-coding-agent": "^0.52.9",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@sinclair/typebox": "^0.34.48",
    "@tavily/core": "^0.7.1",
    "better-sqlite3": "^12.6.2",
    "chalk": "^5.6.2",
    "highlight.js": "^11.11.1",
    "ink": "^5.2.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "isomorphic-git": "^1.36.3",
    "marked": "^11.2.0",
    "react": "^18.3.1",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.2.2",
    "@types/react": "^18.3.28",
    "fast-check": "^4.5.3",
    "ink-testing-library": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
